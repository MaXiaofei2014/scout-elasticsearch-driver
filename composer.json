{
  "name": "synergy/scout-elasticsearch-driver",
  "type": "library",
  "description": "The Elasticsearch Driver for Laravel Scout",
  "keywords": [
    "elastic",
    "elasticsearch",
    "driver",
    "engine",
    "laravel",
    "scout",
    "search"
  ],
  "homepage": "https://github.com/odiaseo/scout-elasticsearch-driver",
  "license": "MIT",
  "authors": [
    {
      "name": "Pele Odiase",
      "email": "peleodiase@yahoo.co.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "elasticsearch/elasticsearch": "^5.3",
    "laravel/scout": "^3.0",
    "laravel/framework": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "SynergyScoutElastic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SynergyScoutElastic\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SynergyScoutElastic\\Providers\\ScoutElasticServiceProvider"
      ],
      "aliases": {
        "ElasticClient": "SynergyScoutElastic\\Facades"
      }
    }
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "phpunit/phpunit": "^6.3",
    "mockery/mockery": "^0.9.9",
    "satooshi/php-coveralls": "^1.0"
  }
}
